  
<h3><a class="btn btn-secondary" href="/employee"><i class="fa fa-plus"></i> Create New</a> Employee List</h3>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th>City</th>
            <th>id</th>

            <th></th>
        </tr>
    </thead>

    <tbody>
        <h3>{{try}} </h3>
            {{#each try}}

        <tr>
            <td>{{name}}</td>
             <td>{{email}}</td>
            <td>{{gender}}</td>
               <td>{{city}}</td> 
        <td>{{_id}}</td> 


            <td>
                <a href="/employee/{{_id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/employee/delete/{{_id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
         {{/each}}

        </tbody>
</table>